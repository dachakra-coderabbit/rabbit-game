{
  "expo": {
    "name": "Rabbit Jump",
    "slug": "rabbit-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#87CEEB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rabbithop.game"
    },
    "android": {
      "package": "com.rabbithop.game"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "rabbit-game"
        }
      ]
    ]
  }
}
